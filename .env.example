# Appwrite Configuration for NaturalSignals-WireScope
APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1
APPWRITE_PROJECT_ID=your_project_id
APPWRITE_API_KEY=your_api_key
APPWRITE_DB_ID=wirescope_db_id
APPWRITE_PROJECTS_COLLECTION=wirescope_projects
